-- This is for redhat linux 9
1. Create a folder /u02 as root

2. Download preinstall package for rhel 9
wget https://yum.oracle.com/repo/OracleLinux/OL9/appstream/x86_64/getPackage/oracle-database-preinstall-23ai-1.0-2.el9.x86_64.rpm

3. dnf -y install oracle-database-preinstall-23ai-1.0-2.el9.x86_64.rpm

4. Download oracle 23ai for rhel 9
wget https://download.oracle.com/otn-pub/otn_software/db-free/oracle-database-free-23ai-1.0-1.el9.x86_64.rpm

5. install oracle 23ai software
dnf -y install oracle-database-free-23ai-1.0-1.el9.x86_64.rpm